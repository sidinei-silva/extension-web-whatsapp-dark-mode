{
  "manifest_version": 2,
  "name": "Dark Mode Web Whatsapp",
  "version": "1.0",
  "description": "Habilita modo dark no web whatsapp",
  "browser_action": {
    "default_title": "Dark Mode Web Whatsapp",
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "permissions": [
    "storage",
    "https://web.whatsapp.com/"
  ]

}